<%- include('./partials/headers'); -%>
  
       
<form action="'/developers/<%=data.id%>?_method=PUT" method="POST" enctype="multipart/form-data">
    <div class="form-group">
      <label  for="exampleInputEmail1">Email address</label>
      <input type="email" name="email" class="form-control" id="Email" aria-describedby="emailHelp" placeholder="Enter email">
      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
      <label  for="password">Password</label>
      <input name="password" type="password" class="form-control" id="password" placeholder="Password">
    </div>

    <div class="form-group">
        <label  for="userName">user name</label>
        <input name="userName" type="text" class="form-control" id="userName" placeholder="user name">
      </div>
      <div class="form-group">
        <label  for="phone">phone</label>
        <input name="phone" type="number" class="form-control" id="phone" placeholder="Phone">
      </div>

    
      <div class="form-group">
        <label  for="description">description</label>
        <input name="description" type="number" class="form-control" id="description" placeholder="give your detailed description">
      </div>
      

      <div class="form-group">
        <label  for="projects">project images</label>
        <input name="projects" type="number" class="form-control" id="projects" placeholder="images">
      </div>
      
      
    <button type="submit" class="btn btn-primary">register</button>
  </form>



<%- include('./partials/footer') ; -%>
